[% META title = 'My Blog' %]

[% FOREACH collection IN collections %]
 <div>
  <h2>[% collection.title %]</h2>
  [% FOREACH entry IN collection.entries %]
   <div class="entry">
    <h3>[% entry.title %]</h3>
    <div class="content">[% entry.content %]</div>
    <div class="updated">[% entry.updated %]</div>
   </div>
  [% END %]
 </div>
[% END %]
